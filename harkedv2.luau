		_IsAttempt = true,
		Then = Then,
		Catch = Catch,
		Retry = Retry,
		Success = Success,
		Arguments = Arguments,
		Stack = debug.traceback(),
		LastArguments = { ... },
		Hops = (not Success) and { Function } or nil,
		RetryCount = 0,
		Start = true

